{#
/**
 * @file
 * Default theme implementation for a Culturefeed search pager.
 *
 * Available variables:
 * - pager: The search pager
 *
 * @see template_preprocess_culturefeed_agenda_search_pager()
 *
 * @ingroup themeable
 */
#}

{%
  set classes = [
    'culturefeed-search-pager',
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {{ pager }}
</div>
